{
    "name": "acmephp/ssl",
    "description": "Tools to manipulate certificates.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/acmephp/ssl",
    "keywords": [
        "certificate",
        "https",
        "acmephp",
        "ssl",
        "openssl",
        "CSR",
        "x509"
    ],
    "authors": [
        {
            "name": "Titouan Galopin",
            "email": "galopintitouan@gmail.com",
            "homepage": "http://titouangalopin.com"
        },
        {
            "name": "Jérémy Derussé",
            "homepage": "https://twitter.com/jderusse"
        }
    ],
    "autoload": {
        "psr-4": {
            "AcmePhp\\Ssl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcmePhp\\Ssl\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "ext-openssl": "*",
        "webmozart/assert": "^1.0",
        "container-interop/service-provider": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.22",
        "sllh/php-cs-fixer-styleci-bridge": "^1.5.0",
        "puli/cli":"^1.0-beta10",
        "puli/composer-plugin": "^1.0-beta10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
